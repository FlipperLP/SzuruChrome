    "node-sass": "^4.14.1",
    "sass-loader": "^7.3.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.7",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.11",
    "web-ext-types": "^2.1.0",
